{
  "name": "queue-simulation-data-access-store",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/queue-simulation/data-access/store/src",
  "projectType": "library",
  "tags": ["type:data-access"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/queue-simulation/data-access/store",
        "main": "libs/queue-simulation/data-access/store/src/index.ts",
        "tsConfig": "libs/queue-simulation/data-access/store/tsconfig.lib.json",
        "assets": ["libs/queue-simulation/data-access/store/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/queue-simulation/data-access/store/jest.config.ts"
      }
    }
  }
}
